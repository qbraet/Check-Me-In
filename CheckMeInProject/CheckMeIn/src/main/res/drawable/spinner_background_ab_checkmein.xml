<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false"
        android:drawable="@drawable/spinner_ab_disabled_checkmein" />
    <item android:state_pressed="true"
        android:drawable="@drawable/spinner_ab_pressed_checkmein" />
    <item android:state_pressed="false" android:state_focused="true"
        android:drawable="@drawable/spinner_ab_focused_checkmein" />
    <item android:drawable="@drawable/spinner_ab_default_checkmein" />
</selector>
